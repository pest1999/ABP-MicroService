<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>BoxApp</RootNamespace>
    <Version>1.0.1.2</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Cmtthe</Authors>
    <Company>Cmtthe</Company>
    <Product>BoxApp</Product>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageId>Pest.BoxApp.Domain</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="BaseData\**" />
    <EmbeddedResource Remove="BaseData\**" />
    <None Remove="BaseData\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Localization\BoxApp\cs.json" />
    <None Remove="Localization\BoxApp\en.json" />
    <None Remove="Localization\BoxApp\zh-Hans.json" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Localization\BoxApp\cs.json" />
    <EmbeddedResource Include="Localization\BoxApp\en.json" />
    <EmbeddedResource Include="Localization\BoxApp\zh-Hans.json">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.AuditLogging.Domain" Version="3.1.0" />
    <PackageReference Include="Volo.Abp.Ddd.Domain" Version="3.1.0" />
    <PackageReference Include="Volo.Abp.EntityFrameworkCore" Version="3.1.0" />
    <PackageReference Include="Volo.Abp.Localization" Version="3.1.0" />
    <PackageReference Include="Volo.Abp.SettingManagement.Domain" Version="3.1.0" />
    <PackageReference Include="Volo.Abp.Validation" Version="3.1.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Models\" />
  </ItemGroup>

</Project>
